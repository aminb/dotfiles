Interface=wlp3s0
Connection=wireless
Security=wpa-configsection
IP=dhcp
WPAConfigSection=(
    'ssid="AirYorkPLUS"'
    'ca_cert="/etc/ssl/certs/GlobalSign_Root_CA.pem"'
    'key_mgmt=WPA-EAP'
    'eap=PEAP'
    'identity="dedacted"'
    'password="dedacted"'
    'priority=1'
    'phase2="auth=MSCHAPV2"'
)
